{
  "name": "vue-live-island",
  "version": "0.1.0",
  "description": "Dynamic Island for Vue",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flyi/vue-live-island.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "nuxt",
    "android",
    "apple",
    "ios",
    "island",
    "dynamic-island",
    "live-island",
    "vue-live-island"
  ],
  "author": "flyi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flyi/vue-live-island/issues"
  },
  "homepage": "https://github.com/flyi/vue-live-island#readme",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.17.17",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.0.6",
    "prettier": "^3.0.3",
    "typescript": "~5.2.0",
    "vite": "^4.4.9",
    "vue-tsc": "^1.8.11"
  }
}
